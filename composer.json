{
    "name": "gungcahyadipp/channex-open-channel",
    "description": "PHP package for Channex Open Channel API integration. Supports standalone PHP 8.2+ and Laravel.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Gung Cahyadi",
            "email": "gungcahyadipp@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "GungCahyadiPP\\ChannexOpenChannel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GungCahyadiPP\\ChannexOpenChannel\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "GungCahyadiPP\\ChannexOpenChannel\\Laravel\\ChannexServiceProvider"
            ],
            "aliases": {
                "Channex": "GungCahyadiPP\\ChannexOpenChannel\\Laravel\\Facades\\Channex"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}